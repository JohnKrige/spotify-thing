{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars":"warn",
        "no-path-concat":"off",
        "arrow-parens": ["error", "as-needed"],
        "object-curly-newline": "off",
        "new-cap":"off",
        "max-len":"off",
        "no-restricted-syntax": "off"
    },
    "extends": ["airbnb"]
}
